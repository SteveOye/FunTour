<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.funtour">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_foreground"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LocationDetails"
            android:label="@string/ld"
            android:parentActivityName=".ViewPager">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".ViewPager" />
        </activity>
        <activity
            android:name=".EventDetails"
            android:label="@string/ed"
            android:parentActivityName=".ViewPager">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".ViewPager" />
        </activity>
        <activity android:name=".ViewPager" />
        <activity
            android:name=".MuseumDetails"
            android:label="@string/md"
            android:parentActivityName=".ViewPager">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".ViewPager" />
        </activity>
        <activity android:name=".WeatherCondition"
            android:label="Weather"
            android:parentActivityName=".ViewPager">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".ViewPager" />
        </activity>
    </application>

</manifest>